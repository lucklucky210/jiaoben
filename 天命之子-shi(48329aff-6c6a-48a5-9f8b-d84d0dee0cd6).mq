Function runRaid()
    timeRecord = TickCount()
    targetX = -1
    targetY = -1
    startY = 0
    lastY = 0
    count = 0
    ticketNum = -1
    srMode = True
    //是否在首页
    result = isHome()
    TracePrint "ishome:",result
    If result And True Then 
        TracePrint "点击屏幕raid位置或者wb位置"
        Touch 955, 969, 100
    Else 
        //不在raid界面
        result = isSR()
        TracePrint "isRaid:",result
        If result = False Then 
            reset =True
            Exit Function
        End If
    End If
		
    Dim ticketstr = myOcrText2ticketNum()
    If IsNull(ticketstr) = False And ticketstr <> "" Then 
        ticketNum = CInt(ticketstr)
    End If
		
    //没票时退出
    If ticketNum = 0 Then 
        Delay 1000
        Exit Function
    End If
		
    clickNormalRefresh 
		
    findshiInNormalMode 
		
    If targetY > srNormalBossBottomY Then 
        lastY =0
        targetY = -1
        targetX = -1
       	
    End If
    TracePrint "normal模式下找到屎:",isShi	
    If targetY = -1 And targetX = -1 Then 
        TracePrint "normal模式下没有找到目标"
        //低于5票时只打屎
        If ticketNum <> -1 And ticketNum <= shiCount Then 
            //5秒刷新一次，找屎
            Delay 1000
            Exit Function
        Else 
            //大于5票才打ssr
            If ticketNum > shiCount Then 
                result= findSr_help()
                findBossInSrMode 
                If srMode Then 
                    If targetY = -1 And targetX = -1 Then 
                        TracePrint "sr模式下没有找到boss"
                        clickWhiteQueRen 
                    End If
                Else 
                    targetX = -1
                    targetY = -1
                End If	
            	
                If targetY > srDialogBossBottomY Then 
                    lastY =0
                    targetY = -1
                    targetX = -1
				
                End If
            End If
        End If 
    End If
		
    If targetY <> -1 Then 
        //进入准备页面
        TracePrint "进入准备页面"
        Touch 100, targetY, 100
		
        loading
        Dim daResult = daRaid()
        loading
        If daResult And True Then 
            //等待打完
            dawan() 
			
        Else 
            // 没打成,返回sr界面
            clickWhiteQueRen 
			
        End If
    End If
	
   
End Function






