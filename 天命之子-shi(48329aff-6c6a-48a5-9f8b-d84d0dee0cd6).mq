//正在加载中
Function loading()
    Rem ld
    Dim intX =-1 ,intY =-1
    Dim tempX = -1,tempY = -1
    Dim num = 0
    //捕获请求开始
    //请求时间短，最低延时5秒
    TracePrint "等待loading"
    While intX = -1 And intY = -1 And num <5
        FindPic 430, 1743, 672, 1865, "Attachment:loading.png", "000000", 0, 0.9, intX, intY
        num = num + 1
        //		TracePrint "检测刷新条xy",intX,intY
        Delay 1000
    Wend
    //捕获请求结束
    //请求超时 
    TracePrint "等待loading超时"
    While intX > -1 And intY > -1
        TracePrint "检测是否超时,转圈中"
        FindPic 430, 1743, 672, 1865, "Attachment:loading.png", "000000", 0, 0.9, intX, intY
        Delay 1000
        If intX = -1 And intY = -1 Then 
            TracePrint "停止转圈,判断超时弹框"
            FindPic 0, 0, 0, 0, "Attachment:yes.png", "000000", 0, 0.9, tempX, tempY
            TracePrint tempX
            TracePrint tempY
            If tempX > -1 And tempY > -1 Then 
                TracePrint "超时-点击是按钮"
                Touch tempX + 50, tempY + 50, 100
                Delay 1000
                Goto ld
            End If
        End If
    Wend
	
End Function

