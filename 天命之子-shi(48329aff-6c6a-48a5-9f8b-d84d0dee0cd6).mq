        If tempX > -1 And tempY > -1 Then
            tempX =tempX +50
            tempY = tempY + 50
        ElseIf tempX = -1 And tempY = -1 Then
            FindStr 460, 1483, 644, 1566, "rank", "FFFFFF|DADDE1", 0.9, tempX, tempY
        End If
        	
        If tempX = -1 And tempY =-1  Then 
            FindPic 837,1174,1079,1564,"Attachment:test666.png","000000",0,0.9,tempX,tempY
        End If        
        Delay 1000
    Wend
    TracePrint "返回成功"
    //    If tempX > -1 And tempY > -1 Then
    //        TracePrint tempX
    //        TracePrint tempY
    //        Touch tempX, tempY, 100
    //    End If
	
