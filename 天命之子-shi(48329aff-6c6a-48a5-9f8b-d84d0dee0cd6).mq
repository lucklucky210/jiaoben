        //进入战斗后提示打完
        FindPic 345, 999, 736, 1143, "Attachment:test888.png", "000000", 0, 0.9, tempX, tempY
		
        If tempX > -1 And tempY > -1 Then
            tempX =tempX +50
            tempY = tempY + 50            
        ElseIf tempX = -1 And tempY = -1 Then
