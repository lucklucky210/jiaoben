//正在加载中
Function loading()
    Dim intX =-1 ,intY =-1
    Dim tempX = -1,tempY = -1
    Dim num = 0
    Rem ld
    intX = -1
    intY = -1
    tempX = -1
    tempY = -1
    num = 0
    //捕获请求开始
    TracePrint "等待loading"
    While intX = -1 And intY = -1 And num <10
        FindPic 347, 1217, 376, 1228, "Attachment:loading_2.png", "000000", 0, 0.8, intX, intY
        num = num + 1
        TracePrint "检测刷新条xy",intX,intY
        Delay 200
    Wend
    //捕获请求结束
    //请求超时 
    TracePrint "等待loading超时"
    While intX > -1 And intY > -1
