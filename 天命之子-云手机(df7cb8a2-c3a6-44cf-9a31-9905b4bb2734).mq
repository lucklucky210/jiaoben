//app是否在运行
Function appRun()
    isAppRunning = -1
    arr=GetRunningApp() //获取所有应用程序包名并将它们合并到arr数组中保存
    For Each n In arr
        If 0 = StrComp(n, "com.stairs.destinychild", 1) Then 
            RunApp "com.stairs.destinychild"
            isAppRunning = 0
            runRaid
            TracePrint "app 已开启,执行raid"
        End If
    Next
	
End Function

Function startupAPP()
    RunApp "com.stairs.destinychild"

    loading 
	
    startup()
    clickDaily
    clickNotice 
	
    loading 
	
    While count < 10
        KeyPress "Back"
        count = count + 1
        Delay 1000
    Wend
    noBtn
    count = 0
    loading 
	
	
End Function

