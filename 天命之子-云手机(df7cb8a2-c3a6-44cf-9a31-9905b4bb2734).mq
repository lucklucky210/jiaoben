Function runRaid()

	Rem raidFunction
    timeRecord = TickCount()
    targetX = -1
    targetY = -1
    startY = 0
    lastY = 0
    count = 0
    ticketNum = -1
    srMode = True
    isShi = False
    condition_querenX = -1
    condition_querenY = -1
    condition_timeX = -1
    condition_timeY = -1
    condition_weicanjiaX = -1
    condition_weicanjiaY= -1
    tempbattlpauseinshi = battlpauseinshi
    //是否在首页
    result = isHome()
    TracePrint "ishome:",result
    
    If result And True Then 
        TracePrint "点击屏幕raid位置或者wb位置"
        Touch 632, 646, 100
        loading 
         //在raid界面
        result = isSR()
        TracePrint "isRaid:",result
         If result = False Then 
            reset =True
            Exit Function
        End If
    Else 
        //在raid界面
        result = isSR()
        TracePrint "isRaid:",result
        If result = False Then 
            reset =True
            Exit Function
        End If
    End If
		
    Dim ticketstr = myOcrText2ticketNum()
    
    If ticketstr Then 
        ticketNum = CInt(ticketstr)
    Else 
        ticketNum = 0
    End If
		
    //没票时退出
    If ticketNum = 0 Then 
        Delay 1000
        Exit Function
    End If
		
	//设定展示顺序
	result = isCondition_OFF()
	
	If result And True Then 
			
		While condition_querenX = -1 And condition_querenY = -1 And count < 10
			//点击设定按钮
			Touch 622, 433, 200
			TracePrint "点击设定按钮"
			Delay 500
			FindPic 268,938,453,1014, "Attachment:condition_queren_2.png", "000000", 0, 0.6, condition_querenX, condition_querenY
			count = count + 1
		Wend
		//点击设定按钮可能受到loading干扰导致点击失败
		If count = 10 And condition_querenX = -1 And condition_querenY = -1 Then 
			TracePrint "点击设定按钮可能受到loading干扰导致点击失败,检测loading"
			loading 
			Touch 622, 433, 200
			TracePrint "点击设定按钮"
			Delay 500
			count=0
			While condition_querenX = -1 And condition_querenY = -1 And count <20
				FindPic 268,938,453,1014, "Attachment:condition_queren_2.png", "000000", 0, 0.6, condition_querenX, condition_querenY
				count = count + 1
				Delay 200	
			Wend
		End If
		count = 0
		TracePrint "条件确认按钮XY：",condition_querenX,condition_querenY
		If condition_querenX = -1 And condition_querenY = -1 Then 
			TracePrint "未找到条件确认按钮 ，流程错误，重置"
			Goto raidFunction
		End If
		
		//设定时间
		While condition_timeX = -1 And condition_timeY = -1 And count < 20
			FindPic 362, 1061, 513, 1154, "Attachment:condition_time_2.png", "000000", 0, 0.6, condition_timeX, condition_timeY
			count = count + 1
			Delay 200	
		Wend
		TracePrint "设定时间按钮XY:",condition_timeX,condition_timeY
		//没找到设定时间按钮
		If count = 20 And condition_timeX = -1 And condition_timeY = -1 Then 
			loading 
			FindPic 362, 1061, 513, 1154, "Attachment:condition_time_2.png", "000000", 0, 0.6, condition_timeX, condition_timeY
		End If
		count = 0
		If condition_timeX = -1 And condition_timeY = -1 Then 
			TracePrint "没找到设定时间按钮，重置"
			Goto raidFunction
		End If
		
		//点击设定时间按钮
		While condition_timeX > -1 And condition_timeY > -1 And count <20
			Touch condition_timeX + 50, condition_timeY + 30, 200
			count = count + 1
			Delay 200
			FindPic 362, 1061, 513, 1154, "Attachment:condition_time_2.png", "000000", 0, 0.6, condition_timeX, condition_timeY
		Wend
		//点了设定时间之后按钮无变化
		If count = 20 And condition_timeX > -1 And condition_timeY > -1 Then 
			loading 
			Touch condition_timeX + 50, condition_timeY + 30, 200
			Delay 200
			FindPic 362, 1061, 513, 1154, "Attachment:condition_time_2.png", "000000", 0, 0.6, condition_timeX, condition_timeY
		End If
		count = 0
		If condition_timeX > -1 And condition_timeY > -1 Then 
			TracePrint "设定时间按钮点击无反应，重置"
			Goto raidFunction
		End If
		
		Delay 1000
		//设定-<未打过>
		While condition_weicanjiaX = -1 And condition_weicanjiaY = -1 And count <20
			FindPic 352,1201,500,1269, "Attachment:condition_weicanjia_2.png", "000000", 0, 0.6, condition_weicanjiaX, condition_weicanjiaY
			Delay 200
			count = count + 1
		Wend
		TracePrint "<未打过设定按钮>XY:",condition_weicanjiaX,condition_weicanjiaY
		//判断是否loading导致未找到按钮
		If count = 20 And condition_weicanjiaX = -1 And condition_weicanjiaY = -1 Then 
			TracePrint "设定时间后刷新超时,走loading后再读取一次<未打过按钮>坐标"
			loading 
			FindPic 352,1201,500,1269, "Attachment:condition_weicanjia_2.png", "000000", 0, 0.6, condition_weicanjiaX, condition_weicanjiaY
			TracePrint "<未打过设定按钮>XY:",condition_weicanjiaX,condition_weicanjiaY
		End If
		count = 0
		
		If condition_weicanjiaX = -1 And condition_weicanjiaY = -1 Then 
			TracePrint "未找到<未打过按钮>，重置"
			Goto raidFunction
		End If
		// 当勾选完时间后，确保<未打过按钮> 不再是未勾选状态 ，可能是loading状态或者被勾选状态 
		While condition_weicanjiaX > -1 And condition_weicanjiaY > -1 And count <20 
			Touch condition_weicanjiaX + 50, condition_weicanjiaY + 30, 200
			count = count+1
			Delay 200
			FindPic 352,1201,500,1269, "Attachment:condition_weicanjia_2.png", "000000", 0, 0.6, condition_weicanjiaX, condition_weicanjiaY
		Wend
		//超过限制时间，<未打过按钮>没有被点击成功,排除loading导致没点击成功(假定loading能识别到按钮)
		If count = 20 And condition_weicanjiaX > -1 And condition_weicanjiaY > -1 Then 
			loading 
			Touch condition_weicanjiaX + 50, condition_weicanjiaY + 30, 200
			Delay 200
			FindPic 352,1201,500,1269, "Attachment:condition_weicanjia_2.png", "000000", 0, 0.6, condition_weicanjiaX, condition_weicanjiaY
		End If	
		count = 0
		
		If condition_weicanjiaX > -1 And condition_weicanjiaY > -1 Then 
			TracePrint "点击<未打过按钮>无反应，重置"
			Goto raidFunction
		End If
		Delay 200
		//都勾选完毕之后，确保确定按钮只会被点击一次
		Touch condition_querenX + 50, condition_querenY + 30, 200
		Delay 200
		FindPic 268, 938, 453, 1014, "Attachment:condition_queren_2.png", "000000", 0, 0.6, condition_querenX, condition_querenY
		If condition_querenX > -1 And condition_querenY > -1 Then 
			loading 
			Touch condition_querenX + 50, condition_querenY + 30, 200
			Delay 200
			FindPic 268, 938, 453, 1014, "Attachment:condition_queren_2.png", "000000", 0, 0.6, condition_querenX, condition_querenY
		End If
		
		If condition_querenX > -1 And condition_querenY > -1 Then 
			TracePrint "点击条件设定确定按钮无反应，重置"
			Goto raidFunction
		End If
		//勾选完条件后，找boss出现障碍
		
//		loading
	Else 
		 clickNormalRefresh 	
			
	End If  
		
    findshiInNormalMode 
		
    If targetY > srNormalBossBottomY Then 
        lastY =0
        targetY = -1
        targetX = -1
       	
    End If
    
    TracePrint "normal模式下找到屎:",isShi	
    
    If targetX = -1 And targetY = -1 Then
    
        TracePrint "normal模式下没有找到目标"
        //低于5票时只打屎
        If ticketNum <> -1 And ticketNum <= shiCount Then 
            //5秒刷新一次，找屎
            Delay 1000
            Exit Function
        Else 
            //大于5票才打ssr
            If ticketNum > shiCount Then 
                result = findSr_help()
                TracePrint "sr支援标记",result
                If srNomal <> 2 Then 
                    srMode = False                   
                Else 
                    findBossInSrMode 
                End If
               
                If srMode Then 
                    If targetY = -1 And targetX = -1 Then 
                        TracePrint "sr模式下没有找到boss"
                        clickWhiteQueRen 
                    End If
                    
                    If targetY > srDialogBossBottomY Then 
                    	lastY =0
                    	targetY = -1
                    	targetX = -1
               		End If
               		
                Else 
                    targetX = -1
                    targetY = -1
                    
                    clickNormalRefresh 
                    
                    findshiInNormalMode 
                    
                    If targetY > srNormalBossBottomY Then 
        				lastY =0
        				targetY = -1
        				targetX = -1
       					
    				End If
    				//如果好友车没有找到，就打sr
    				If srNomal = 0 And ticketNum <= 8 Then 
    						result = False
    				ElseIf srNomal = 1 And ticketNum <= shiCount Then
    						result = False
    				Else 
    					result = True
    				End If
    				If result And targetX = -1 And targetY = -1 Then 
    					findBossInSrMode 
    					If targetY = -1 And targetX = -1 Then 
                        	TracePrint "sr模式下没有找到boss"
                        	clickWhiteQueRen 
                    	End If
                    		
                    	If targetY > srDialogBossBottomY Then 
                    		lastY =0
                    		targetY = -1
                    		targetX = -1
               			End If
    					
    				End If
    				 
                End If	
                
            End If
            
        End If
        
    End If
		
    If targetY <> -1 Then 
        //进入准备页面
        TracePrint "进入准备页面,本次寻找目标费时:",(TickCount()-timeRecord)\1000,"秒"
        timeRecord = TickCount()
        Touch 100, targetY, 100
		
        loading
        Dim daResult = daRaid()
        loading
        If daResult And True Then 
            //等待打完
            If isShi Then 
            	totalShiCount = totalShiCount+1	
            End If
            dawan() 
			
        Else 
            // 没打成,返回sr界面
            clickWhiteQueRen 
			
        End If
    End If
	
	If reset Then 
		Exit Function
	End If
	Goto raidFunction
   
End Function


//raid界面，条件是否未设置
Function isCondition_OFF
	Dim ocrchar
	ocrchar = Ocr(513,432,561,460, "808080", 0.8)
	If StrComp("OFF", ocrchar) = 0 Then 
		isCondition_OFF = True
	Else 
		isCondition_OFF = False
	End If
End Function




